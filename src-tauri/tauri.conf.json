{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jvm-ram-cost",
  "version": "1.0.4",
  "identifier": "jvmram",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../frontend-dist"
  },
  "app": {
    "windows": [
      {
        "title": "jvm-ram-cost",
        "width": 1800,
        "height": 1200
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../dist-app-backend/build/minified/jvm-ram-cost.jar": "jvm-ram-cost.jar"
    }
  }
}
